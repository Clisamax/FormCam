{
	"expo": {
		"name": "FormCam",
		"slug": "FormCam",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "formcam",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/images/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.clisamax.FormCam",
			"runtimeVersion": "1.0.0",
			"infoPlist": {
				"NSCameraUsageDescription": "Este aplicativo precisa acessar a c√¢mera para tirar fotos."
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true,
			"runtimeVersion": {
				"policy": "appVersion"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "e25bbd2d-b867-49fd-b4b4-101bb9f50ab2"
			}
		},
		"updates": {
			"url": "https://u.expo.dev/e25bbd2d-b867-49fd-b4b4-101bb9f50ab2"
		}
	}
}
